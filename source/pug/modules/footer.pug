footer.footer
  .footer__wrapper
    .footer__social.social
      a(href="https://selectel.ru/" target="_blank")
        img.footer__logo.logo(src="img/LOGO.png" width="112" height="24" alt="Логотип")
      ul.social__list
        li.social__item.social__item--vk
          a.social__link(href="https://vk.com/selectel" target="_blank" aria-label="ВКонтакте")
            svg.social__icon.social__icon--vk(width="22" height="22")
              use(xlink:href="#icon-vk")
        li.social__item.social__item--twit
          a.social__link(href="https://twitter.com/selectel" target="_blank" aria-label="Twitter")
            svg.social__icon.social__icon--twit(width="22" height="22")
              use(xlink:href="#icon-twit")
        li.social__item.social__item--fb
          a.social__link(href="https://www.facebook.com/selectel/" target="_blank" aria-label="Facebook")
            svg.social__icon.social__icon--fb(width="22" height="22")
              use(xlink:href="#icon-fb")
        li.social__item.social__item--insta
          a.social__link(href="https://www.instagram.com/selectel.ru/" target="_blank" aria-label="Instagram")
            svg.social__icon.social__icon--insta(width="22" height="22")
              use(xlink:href="#icon-insta")
    .footer__nav.nav
      ul.nav__list
        li.nav__item.nav__item--host
          a.nav__link(href="https://selectel.ru/" target="_blank") Серверы
        li.nav__item.nav__item--equipment
          a.nav__link(href="https://selectel.ru/" target="_blank") Оборудование
        li.nav__item.nav__item--data
          a.nav__link(href="https://selectel.ru/" target="_blank") Дата-центры
        li.nav__item.nav__item--blog
          a.nav__link(href="https://selectel.ru/" target="_blank") Блог
        li.nav__item.nav__item--cloud
          a.nav__link(href="https://selectel.ru/" target="_blank") Облако
        li.nav__item.nav__item--decision
          a.nav__link(href="https://selectel.ru/" target="_blank") Решения
        li.nav__item.nav__item--company
          a.nav__link(href="https://selectel.ru/" target="_blank") О компании
        li.nav__item.nav__item--office
          a.nav__link(href="https://selectel.ru/" target="_blank") Офисы
